<!DOCTYPE html>
<html lang="en">
  <body>
    <h1><%= title %></h1>
    <ul>
      <% if (users && users.length > 0) {%> <% users.forEach(user => { %>
      <li>
        ID: <%= user.id %>, 
        Name: <%= user.firstName %> <%= user.lastName
        %>,
        Email: <%= user.email %>, 
        Age: <%= user.age %>, 
        Bio: <%= user.bio %>
      </li>
      <% }); %> 
      <% } else { %>
      <p>No users found.</p>
      <% } %>
    </ul>
  </body>
</html>
